				</TCEforms>
			</numCheckMaxInterval>
			<numDefaultNights>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.numDefaultNights</label>
					<config>
						<type>input</type>
						<size>2</size>
						<max>2</max>
						<default>2</default>
					</config>
				</TCEforms>
			</numDefaultNights>
			<numDefaultPersons>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.numDefaultPersons</label>
					<config>
						<type>input</type>
						<size>2</size>
						<max>2</max>
						<default>2</default>
					</config>
				</TCEforms>
			</numDefaultPersons>
			<showPersonsSelector>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.showPersonsSelector</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</showPersonsSelector>
<!--			<detailInfosUID>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.detailInfosUID</label>
					<config>
						<type>input</type>
						<size>3</size>
						<max>3</max>
						<default></default>
					</config>
				</TCEforms>
			</detailInfosUID>-->
		</el>
	</ROOT>
</sheetAvailabilityOptions>

<sheetBookingOptions>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.sheetBookingOptions</sheetTitle>
			<cshFile>LLL:EXT:ab_booking/locallang_csh.xml</cshFile>
		</TCEforms>
		<type>array</type>
		<el>
			<useSwiftMailer>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.useSwiftMailer</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</useSwiftMailer>
			<EmailAddress>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.EmailAddress</label>
					<config>
						<type>input</type>
						<size>60</size>
						<max>60</max>
						<default></default>
					</config>
				</TCEforms>
			</EmailAddress>
			<EmailRealname>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.EmailRealname</label>
					<config>
						<type>input</type>
						<size>60</size>
						<max>60</max>
						<default></default>
					</config>
				</TCEforms>
			</EmailRealname>
			<textSayThankYou>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.textSayThankYou</label>
					<config>
						<type>text</type>
						<cols>60</cols>
						<rows>5</rows>
						<wrap>off</wrap>
						<default>Thank you for your request.</default>
					</config>
<!--
					<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
-->
				</TCEforms>
			</textSayThankYou>
			<textConfirmEmail>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.textConfirmEmail</label>
					<config>
						<type>text</type>
						<cols>60</cols>
						<rows>5</rows>
						<wrap>off</wrap>
						<default>Email to You.</default>
					</config>
<!--
					<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
-->
				</TCEforms>
			</textConfirmEmail>
		</el>
	</ROOT>
</sheetBookingOptions>

</sheets>
</T3DataStructure>

