<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
  <sheets>
<sheetGeneralOptions>
       <ROOT>
	    <TCEforms>
		    <sheetTitle>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.sheetGeneralOptions</sheetTitle>
	    </TCEforms>
		<type>array</type>
		<el>
		<what_to_display>
			<TCEforms>
				<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.what_to_display</label>
				<config>
					<type>select</type>
					<items type="array">
						<numIndex index="0" type="array">
							<numIndex index="0">CALENDAR</numIndex>
							<numIndex index="1">CALENDAR</numIndex>
						</numIndex>
						<numIndex index="1" type="array">
							<numIndex index="0">AVAILABILITY CHECK</numIndex>
							<numIndex index="1">AVAILABILITY CHECK</numIndex>
						</numIndex>
						<numIndex index="2" type="array">
							<numIndex index="0">BOOKING</numIndex>
							<numIndex index="1">BOOKING</numIndex>
						</numIndex>
						<numIndex index="3" type="array">
							<numIndex index="0">CALENDAR LINE</numIndex>
							<numIndex index="1">CALENDAR LINE</numIndex>
						</numIndex>
					</items>
					<size>4</size>
					<maxitems>1</maxitems>
					<minitems>1</minitems>
<!-- 					<multiple>0</multiple> -->
					<selectedListStyle>width:250px</selectedListStyle>
					<itemListStyle>width:250px</itemListStyle>
				</config>
			</TCEforms>
		</what_to_display>
		<ProductID>
			<TCEforms>
 			<exclude>1</exclude>
			<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.ProductID</label>
			<config>
				<type>select</type>
				<maxitems>100</maxitems>
				<size>5</size>
                                <minitems>0</minitems>
 				<itemsProcFunc>tx_abbooking_remote->flexFormListProductIDs</itemsProcFunc>
			</config>
			</TCEforms>
		</ProductID>
		<PIDstorage>
			<TCEforms>
				<exclude>1</exclude>
				<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.PIDstorage</label>
				<config>
					<type>group</type>
					<internal_type>db</internal_type>
					<allowed>pages</allowed>
					<size>1</size>
					<maxitems>1</maxitems>
					<minitems>0</minitems>
					<show_thumbs>1</show_thumbs>
				</config>
			</TCEforms>
		</PIDstorage>
		<PIDbooking>
			<TCEforms>
				<exclude>1</exclude>
				<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.PIDbooking</label>
				<config>
					<type>group</type>
					<internal_type>db</internal_type>
					<allowed>pages</allowed>
					<size>1</size>
					<maxitems>1</maxitems>
					<minitems>0</minitems>
					<show_thumbs>1</show_thumbs>
				</config>
			</TCEforms>
		</PIDbooking>
	</el>
	</ROOT>
</sheetGeneralOptions>


<sheetCalendarOptions>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.sheetCalendarOptions</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<enableCalendarBookingLink>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.enableCalendarBookingLink</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</enableCalendarBookingLink>
			<numMonths>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.numMonths</label>
					<config>
						<type>input</type>
						<size>2</size>
						<max>2</max>
						<default>4</default>
					</config>
				</TCEforms>
			</numMonths>
			<numMonthsCols>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.numMonthsCols</label>
					<config>
						<type>input</type>
						<size>2</size>
						<max>2</max>
						<default>1</default>
					</config>
				</TCEforms>
			</numMonthsCols>
			<numMonthsRows>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.numMonthsRows</label>
					<config>
						<type>input</type>
						<size>2</size>
						<max>2</max>
						<default>2</default>
					</config>
				</TCEforms>
			</numMonthsRows>
			<monthsLineView>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.monthsLineView</label>
					<config>
						<type>check</type>
						<default>0</default>
					</config>
				</TCEforms>
			</monthsLineView>
		</el>
	</ROOT>
</sheetCalendarOptions>

<sheetAvailabilityOptions>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.sheetAvailabilityOptions</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<enableCheckBookingLink>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.enableCheckBookingLink</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</enableCheckBookingLink>
			<numCheckNextMonths>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.numCheckNextMonths</label>
					<config>
						<type>input</type>
						<size>2</size>
						<max>2</max>
						<default>12</default>
					</config>
				</TCEforms>
			</numCheckNextMonths>
			<numCheckMaxInterval>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.numCheckMaxInterval</label>
					<config>
						<type>input</type>
						<size>2</size>
						<max>2</max>
						<default>21</default>
					</config>
				</TCEforms>
			</numCheckMaxInterval>
			<numDefaultNights>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.numDefaultNights</label>
					<config>
						<type>input</type>
						<size>2</size>
						<max>2</max>
						<default>2</default>
					</config>
				</TCEforms>
			</numDefaultNights>
			<numDefaultPersons>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.numDefaultPersons</label>
					<config>
						<type>input</type>
						<size>2</size>
						<max>2</max>
						<default>2</default>
					</config>
				</TCEforms>
			</numDefaultPersons>
			<showPersonsSelector>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.showPersonsSelector</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</showPersonsSelector>
<!--			<detailInfosUID>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.detailInfosUID</label>
					<config>
						<type>input</type>
						<size>3</size>
						<max>3</max>
						<default></default>
					</config>
				</TCEforms>
			</detailInfosUID>-->
		</el>
	</ROOT>
</sheetAvailabilityOptions>

<sheetBookingOptions>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.sheetBookingOptions</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>
			<useSwiftMailer>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.useSwiftMailer</label>
					<config>
						<type>check</type>
						<default>1</default>
					</config>
				</TCEforms>
			</useSwiftMailer>
			<EmailAddress>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.EmailAddress</label>
					<config>
						<type>input</type>
						<size>60</size>
						<max>60</max>
						<default></default>
					</config>
					<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
				</TCEforms>
			</EmailAddress>
			<EmailRealname>
				<TCEforms>
					<exclude>1</exclude>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.EmailRealname</label>
					<config>
						<type>input</type>
						<size>60</size>
						<max>60</max>
						<default></default>
					</config>
					<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
				</TCEforms>
			</EmailRealname>
			<textSayThankYou>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.textSayThankYou</label>
					<config>
						<type>text</type>
						<cols>60</cols>
						<rows>5</rows>
						<wrap>off</wrap>
						<default>Thank you for your request.</default>
					</config>
				</TCEforms>
			</textSayThankYou>
			<textConfirmEmail>
				<TCEforms>
					<label>LLL:EXT:ab_booking/locallang_tca.xml:ab_booking.pi_flexform.textConfirmEmail</label>
					<config>
						<type>text</type>
						<cols>60</cols>
						<rows>5</rows>
						<wrap>off</wrap>
						<default>Email to You.</default>
					</config>
				</TCEforms>
			</textConfirmEmail>
		</el>
	</ROOT>
</sheetBookingOptions>

